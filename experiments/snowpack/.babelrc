{
  "plugins": [
    ["snowpack/assets/babel-plugin.js", {}],
    ["@babel/plugin-transform-react-jsx", { "pragma": "createElement" }],
    ["@babel/plugin-proposal-object-rest-spread"],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ],
  "presets": [
    "@babel/react",
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        },
        "modules": "false",
        "useBuiltIns": false
      }
    ]
  ]
}
